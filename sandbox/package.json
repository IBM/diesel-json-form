    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-tea-cup": "^2.2.3",
    "tea-cup-core": "^2.2.3",
    "tea-pop-core": "^5.0.0",
    "tea-pop-menu": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.25",
