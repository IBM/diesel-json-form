{
  "name": "@diesel-parser/json-form",
  "version": "3.6.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@diesel-parser/json-schema-facade-ts": "^0.11.0",
    "i18next": "^20.6.1",
    "tea-pop-core": "^5.0.0",
    "tea-pop-menu": "^5.0.0"
  },
  "peerDependencies": {
    "@carbon/react": "^1.71.0",
    "@carbon/icons-react": "^11.53.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-tea-cup": "^2.2.3",
    "tea-cup-core": "^2.2.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "12",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.25",
    "jest": "24.9.0",
    "npm-scripts-watcher": "^1.0.2",
    "ts-jest": "24.0.2"
  },
  "scripts": {
    "build": "yarn clean && yarn lint && yarn build:ts && yarn test && yarn build:scss",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "build:scss": "node-sass --importer ../node_modules/node-sass-package-importer/dist/cli.js src/JsonEditor.scss dist/JsonEditor.css",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch": "npm-scripts-watcher",
    "prettify": "prettier --write .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '**/*.{js,ts,tsx}' --fix"
  },
  "watch": {
    "src/**/!(*.test|*.spec).{ts,tsx}": [
      "build:ts"
    ],
    "src/**/*.scss": [
      "build:scss"
    ]
  }
}
